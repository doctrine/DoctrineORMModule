{
    "name":              "doctrine/doctrine-orm-module",
    "description":       "Zend Framework 3 Module that provides Doctrine ORM functionality",
    "type":              "library",
    "license":           "MIT",
    "keywords":          [
        "doctrine",
        "orm",
        "module",
        "zf3"
    ],
    "homepage":          "http://www.doctrine-project.org/",
    "authors":           [
        {
            "name":     "Kyle Spraggs",
            "email":    "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        },
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://marco-pivetta.com/"
        },
        {
            "name":     "Evan Coury",
            "email":    "me@evancoury.com",
            "homepage": "http://blog.evan.pro/"
        },
        {
            "name":  "Guilherme Blanco",
            "email": "guilhermeblanco@hotmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webimpress/DoctrineModule"
        }
    ],
    "require":           {
        "php":                               "^5.6 || ^7.0",
        "doctrine/doctrine-module":          "dev-feature/zf3-support as 2.0",
        "doctrine/orm":                      ">=2.5,<2.7",
        "doctrine/dbal":                     ">=2.4,<2.7",
        "symfony/console":                   "^2.5 || ^3.0",
        "zendframework/zend-mvc":            "^3.0.2",
        "zendframework/zend-servicemanager": "^3.1.0",
        "zendframework/zend-stdlib":         "^3.0.1"
    },
    "require-dev":       {
        "doctrine/data-fixtures":             "^1.2",
        "doctrine/migrations":                "^1.4",
        "phpunit/phpunit":                    "^5.4",
        "squizlabs/php_codesniffer":          "^2.6.2",
        "zendframework/zend-developer-tools": "^1.1",
        "zendframework/zend-i18n":            "^2.7.3",
        "zendframework/zend-modulemanager":   "^2.7.2"
    },
    "suggest":           {
        "zendframework/zend-form":            "if you want to use form elements backed by Doctrine",
        "zendframework/zend-developer-tools": "zend-developer-tools if you want to profile operations executed by the ORM during development",
        "doctrine/migrations":                "doctrine migrations if you want to keep your schema definitions versioned"
    },
    "autoload":          {
        "psr-4": {
            "DoctrineORMModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineORMModuleTest\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "component": "DoctrineORMModule",
            "config-provider": "DoctrineORMModule\\ConfigProvider"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
